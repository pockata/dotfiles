color	activeborder	"#915661"
color	inactiveborder	"#484848"
color	font		"#f2f3f4"
color	selfont		"#f2f3f4"
color	menubg		"#484848"
color	menufg		"#915661"

borderwidth	6
gap		60 15 15 15

sticky	yes

ignore	lemonbar

unbind-key	all

bind-key	4S-r		restart
bind-key	4-space		group-toggle-all
bind-key	4-Return	alacritty
bind-key	4S-Return	nautilus

bind-key	4-1		group-only-1
bind-key	4-2		group-only-2
bind-key	4-3		group-only-3
bind-key	4-4		group-only-4
bind-key	4-5		group-only-5
bind-key	4C-1		group-toggle-1
bind-key	4C-2		group-toggle-2
bind-key	4C-3		group-toggle-3
bind-key	4C-4		group-toggle-4
bind-key	4C-3		group-toggle-5
bind-key	4S-1		window-movetogroup-1
bind-key	4S-2		window-movetogroup-2
bind-key	4S-3		window-movetogroup-3
bind-key	4S-4		window-movetogroup-4
bind-key	4S-5		window-movetogroup-5
bind-key	4-period	group-cycle
bind-key	4-comma		group-rcycle

bind-key	4-w		"rofi -show window"
bind-key	4S-q		window-delete
bind-key	4-f		"maximize.sh"
bind-key	4S-f		window-fullscreen
bind-key	4-s		window-stick
bind-key	4-Tab		window-cycle
bind-key	4S-Tab		window-rcycle

# Mouse bindings
bind-mouse	4-1		window-move
bind-mouse	4-3		window-resize

bind-mouse 4-4          window-raise
bind-mouse 4-5          window-lower

